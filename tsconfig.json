{
	"compilerOptions": {
		"target": "ES5",
		"module": "commonjs",
		"outDir": "./",
		"strict": true,
		"noImplicitAny": true,
		"removeComments": true,
		"preserveConstEnums": true,
		"esModuleInterop": true,
		"declaration": false,
		"pretty":true,
		"types": [
			"node"
		]
	},
	"include": [
	  "assets/*.ts"
	],
	"exclude": [
	  "node_modules",
	  "test"
	]
}